# ===========================================
# Train Routing API - Environment Configuration
# ===========================================
# Copy this file to .env and adjust values

# Application
APP_ENV=dev
APP_SECRET=your-secret-key-min-32-chars-here

# Database
DB_NAME=trainrouting
DB_USER=app
DB_PASSWORD=secret
DB_PORT=5432

# JWT Authentication
JWT_PASSPHRASE=your-jwt-passphrase

# API User (for JWT authentication)
API_USER_PASSWORD=secret

# Frontend
VITE_API_URL=https://localhost/api/v1
VITE_API_USERNAME=api
VITE_API_PASSWORD=secret

# Ports
HTTP_PORT=80
HTTPS_PORT=443
STORYBOOK_PORT=6006
